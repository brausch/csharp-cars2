<!DOCTYPE html>
<html>
<head>
  <meta charset='utf-8'>
  <title>Car List</title>
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
</head>
  <body>
    <h1>Car</h1>
    <ul>
      @foreach(var car in Model)
      {
        <li>@car.GetMake()</li>
        <li>@car.GetModel()</li>
        <li>@car.GetYear()</li>
        <li>@car.GetMileage()</li>
        <li>@car.GetPrice()</li>
        <li>@car.GetDescription()</li>
      }
    </ul>
    <p><a href="/">Add another Car</a></p>
  </body>
</html>
